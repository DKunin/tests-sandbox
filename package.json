{
  "name": "test-sandbox",
  "version": "1.0.0",
  "description": "Testing out tests and stuff in simple sandbox",
  "main": "index.js",
  "scripts": {
    "test": "mocha ./test/*.spec.js --reporter spec  --recursive",
    "test:watch": "mocha ./test/*.spec.js --reporter spec  --recursive --watch",
    "spec": "mocha ./test/*.spec.js -R markdown > ./spec.md",
    "istanbul": "istanbul cover node_modules/.bin/_mocha",
    "istanbul:node": "istanbul cover --include-all-sources bin/run-tests",
    "istanbul:babel": "babel-esnext ./node_modules/babel-istanbul/lib/cli cover --dir ./coverage node_modules/.bin/_mocha",
    "check-coveragen": "istanbul check-coverage"
  },
  "author": "DKunin",
  "license": "MIT",
  "devDependencies": {
    "chai": "3.5.0",
    "istanbul": "0.4.4",
    "mocha": "2.5.3",
    "babel-core": "6.7.4",
    "babel-loader": "6.2.4",
    "babel-plugin-external-helpers-2": "6.3.13",
    "babel-plugin-syntax-class-properties": "6.5.0",
    "babel-plugin-syntax-export-extensions": "6.5.0",
    "babel-plugin-syntax-object-rest-spread": "6.5.0",
    "babel-plugin-transform-class-properties": "6.6.0",
    "babel-plugin-transform-export-extensions": "6.5.0",
    "babel-plugin-transform-object-rest-spread": "6.6.5",
    "babel-plugin-transform-runtime": "6.6.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-es2015-loose": "7.0.0",
    "babel-preset-react": "6.5.0"
  },
  "dependencies": {
    "babel-esnext": "0.2.3",
    "babel-istanbul": "0.11.0"
  }
}
